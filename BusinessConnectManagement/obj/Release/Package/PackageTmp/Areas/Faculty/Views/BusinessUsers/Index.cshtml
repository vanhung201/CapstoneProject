<form class="form" id="dowloadForm" method="post" enctype="multipart/form-data">
    <div class="form-container--small">
        <h1 class="form-heading">Excel Mẫu</h1>
        <div class="row">
            <div class="column">
                <div class="form-group">
                    <label class="form-label">Doanh Nghiệp</label>
                </div>

            </div>
            <div class="column">
                <div class="form-group">
                    <a href="@Url.Action("DownloadFile", "BusinessUsers", new { filePath = "Business.xlsx" })">Business.xlsx</a>
                </div>

            </div>
        </div>


        <div class="form-group">

            <a class="btn btn--confirm">Xác Nhận</a>

        </div>
    </div>

</form>
@*--------------------------Import-------------------------------*@
<form class="form" action="@Url.Action("ImportData", "BusinessUsers")" id="importForm" method="post" enctype="multipart/form-data">
    <div class="form-container--small">
        <h1 class="form-heading">Import Excel</h1>
        <div class="form-group">
            <input type="file" name="postFile" id="excelFile" accept=".csv, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" onchange="checkInputExcel(this)" required class="form-input" />
            <span class="text-danger" id="cv_validation" style="color:red;"></span>
        </div>

        <div class="form-group">
            <a class="btn btn--cancel">Hủy</a>
            <button type="submit" id="submit_edit" class="btn btn--confirm">Xác Nhận</button>
        </div>
    </div>

</form>
