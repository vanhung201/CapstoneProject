@section Scripts {
    <script type="text/javascript">
        function checkInputExcel(input) {
            if (input.files && input.files[0]) {
                var file = input.files[0];
                var fileType = file.type.toLowerCase();
                if (fileType != "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" && fileType != "application/vnd.ms-excel" && fileType != "text/csv") {
                    input.value = "";
                    $('#cv_validation').text('Vui Lòng Chọn File Excel');
                }
            } else {
                input.value = "";
                $('#cv_validation').text('Vui Lòng Chọn File Excel');
            }
        }
        function loadDeleteUser(id) {
            let formDelete = document.getElementById("deleteForm");
            formDelete.classList.add('is-appear')
            document.getElementById('iddele').value = id;
        }
        $('.btn--cancel').on('click', function () {
            $('#importForm').removeClass('is-appear')
        })

        $('.btn--confirm').on('click', function () {
            $('#dowloadForm').removeClass('is-appear')
        })
    </script>
    <script>
        $(function () {
            var url = "@Url.Action("getDataList","BusinessUsers")"
            "use strict";
            (dataTable = $("#table_test").DataTable({
                ajax: {
                    url: url,
                    type: "GET",
                    dataType: "json",
                    dataSrc: "",
                },
                deferRender: !0,
                "order": [[1, "desc"]],
                columns: [
                    { data: null },
                    { data: "id",visible: false},
                    { data: "username"},
                    { data: "buName" },
                    { data: "emailContact" },
                    { data: "contactName" },
                    { data: "contactPhone1" },
                    {
                        data: "status",
                        render: function (data) {
                            if (data == "Đang hoạt động") {
                                return '<span class="text--done">' + data + '</span>'
                            } else {
                                return '<span class="text--cancel">' + data + '</span>'
                            }

                             }
                    },
                    {
                        data: "id",
                        render: function (t, e, a) {
                            return (
                                `
                                <a href="@Url.Action("Details", "BusinessUsers")${t}" id="business_detail" class="business_detail">
                                <div class="btn btn--update">
                                    <i class="btn-icon btn-icon--edit fal fa-edit"></i>
                                </div>
                                </a>
                                <a onclick="loadDeleteUser(${t})">
                                <div class="btn btn--delete">
                                    <i class="btn-icon btn-icon--edit fal fa-times"></i>
                                </div>
                                </a>`
                            );
                        },
                    },
                ],
                initComplete: function () {
                    this.api()
                        .columns(7)
                        .every(function () {
                            var t = this,
                                e = $(
                                    '<select id="UserType" style="margin-right: 8px;border: 1px solid #aaa;border-radius: 3px;padding: 5px;background-color: transparent; margin-left: 3px;" class="form-select text-capitalize mb-md-0 mb-2xx"><option value="">---- Chọn Trạng Thái Để Lọc ----</option></select>'
                                )
                                    .prependTo("#table_test_filter")
                                    .on("change", function () {
                                        var e = $.fn.dataTable.util.escapeRegex($(this).val());
                                        t.search(e ? "^" + e + "$" : "", !0, !1).draw();
                                    });
                            t.data()
                                .unique()
                                .sort()
                                .each(function (t, a) {
                                    t &&
                                        e.append(
                                            '<option value="' +
                                            t +
                                            '" class="text-capitalize">' +
                                            t +
                                            "</option>"
                                        );
                                });
                        })
                }
            }))
            // Create Index column datatable
            dataTable.on("draw.dt", function () {
                dataTable.column(0, { search: "applied", order: "applied" }).nodes().each(function (cell, i) {
                    cell.innerHTML = i + 1;
                });
            });

        });

        $(function () {
            $("#table_test_filter").append($("#filterEvent")).clone();
        });
        $('#table_test').on('click', '.business_detail', function (e) {
            e.preventDefault()
            const form = document.getElementById('form_detail');
        form.classList.add('is-appear');
            const url = $(this).attr('href');
            $.ajax({
                url: url,
                type: "get",
                dataType: 'json',
                success: function (res) {
                    console.log(res)
                    const imgPath = `@Url.Content("~/Uploads/Images/")` + res[0].buLogo
                    const actionUrl = `@Url.Action("Edit","BusinessUsers")` + `/${res[0].id}`
                    $('#ID').val(res[0].id)
                    $('#Username').val(res[0].username)
                    $('#BusinessName').val(res[0].buName)
                    $('#Address').val(res[0].address)
                    $('#Website').val(res[0].website)
                    $('#ContactName').val(res[0].contactName)
                    $('#ContactPhone_1').val(res[0].contactPhone1)
                    $('#ContactPhone_2').val(res[0].contactPhone2)
                    $('#Last_Access').val(res[0].lastAccess)
                    $('#Last_Access2').val(res[0].lastAccess)
                    $('#BusinessPhone').val(res[0].buPhone)
                    $('#EmailContact').val(res[0].emailContact)
                    $('#Fanpage').val(res[0].fanpage)
                    $('#BusinessLogo').val(res[0].buLogo),
                    $('#Password').val(res[0].password),
                    $('#Semester_ID').val(res[0].sem_id)
                    $('#output1').attr('src', `${imgPath}`)
                    $('#output1').css('display','block')
                    if (res[0].status === "Đang hoạt động") {
                        $('select option[value="active"]').attr("selected", "selected");
                    } else {
                        $('select option[value="block"]').attr("selected", "selected");
                    }
                    $('#form_detail').attr('action', `${actionUrl}`)
                }
            })
        })

        $('.btn--cancel').on('click', function () {
            $('#addForm').removeClass('is-appear')
        })
        $('#submitadd').on('click', function () {
            $('#addForm').submit()
        })
        $('.btn--cancel').on('click', function () {
            $('#form_detail').removeClass('is-appear')
        })
        $('#btn--update').on('click', function () {
            $('#form_detail').submit()
        })
        $('.btn--cancel').on('click', function () {
            $('#deleteForm').removeClass('is-appear')
        })
        $('.btn--cancel').on('click', function () {
            $('#importAlert').removeClass('is-appear')
        })
        $('#btn--cancel').on('click', function () {
            $('#importAlert').removeClass('is-appear')
        })

    </script>

    <script>
        $(document).ready(function () {
            $.validator.addMethod("blank", function (value, element) {
                return (!/^\s*$/.test(value));
            }, "Vui lòng nhập dữ liệu.");

            // Initialize the form validation
            $("#addForm").validate();

            // cái này là on blur nè
            $("#addForm input, #addForm input").on("blur", function () {
                $(this).valid();
            });

            // cái này là on change nè
            $("#addForm input, #addForm input").on("change", function () {
                $(this).removeClass("error");
            });
            /*--------------------------update---------------*/
            // Initialize the form validation
            $("#form_details").validate();

            // cái này là on blur nè
            $("#form_detail input, #form_detail input").on("blur", function () {
                $(this).valid();
            });

            // cái này là on change nè
            $("#form_detail input, #form_detail input").on("change", function () {
                $(this).removeClass("error");
            });
        });
        $("#addForm").validate({
            rules: {
                Username: {
                    required: true,
                    minlength: 3,
                    blank: true
                },
                Password: {
                    required: true,
                    minlength: 3,
                    blank: true
                },
                BusinessName: {
                    required: true,
                    minlength: 3,
                    blank: true
                },
                BusinessPhone: {
                    digits: true,
                    minlength: 10,
                    maxlength: 11
                },
                ContactName: {
                    required: true,
                    minlength: 3,
                    blank: true
                },
                ContactPhone_1: {
                    required: true,
                    digits: true,
                    minlength: 10,
                    maxlength: 11
                },
                ContactPhone_2: {
                    digits: true,
                    minlength: 10,
                    maxlength: 11
                },
                EmailContact: {
                    email: true
                },
                logo: {
                    required: true
                }
            },
            messages: {
                Username: {
                    required: "Vui lòng nhập tên đăng nhập.",
                    minlength: "Tên của bạn phải ít nhất 3 kí tự.",
                },
                Password: {
                    required: "Vui lòng nhập nhập mật khẩu.",
                    minlength: "Tên của bạn phải ít nhất 3 kí tự.",
                },
                BusinessName: {
                    required: "Vui lòng nhập tên tên doanh nghiệp.",
                    minlength: "Tên của bạn phải ít nhất 3 kí tự.",
                },
                BusinessPhone: {
                    digits: "Số điện thoại chỉ bao gồm các kí tự số.",
                    minlength: "Số điện thoại phải có ít nhất 10 số.",
                    maxlength: "Số điện thoại chỉ có thể có tối đa 11 số."
                },
                ContactName: {
                    required: "Vui lòng nhập tên người liên hệ.",
                    minlength: "Tên của bạn phải ít nhất 3 kí tự.",
                },
                ContactPhone_1: {
                    required: "Vui lòng nhập số điện thoại.",
