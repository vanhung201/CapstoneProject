            <div class="title-line d-flex justify-center">
                <h3>MOU</h3>
            </div>
            <div class="columns py20">
                @foreach (var item in Model)
                {
                    <div class="column col-4 col-sm-12 col-md-12 col-xl-4 mb10">
                        <div class="item">
                            <div class="zoom-small">
                                <img src="@Url.Content("~/Uploads/Images/" + item.BusinessUser.BusinessLogo)" alt="mou1">
                            </div>
                            <div class="py-2 d-flex align-middle scroll-mobile">
                                <small class="bold-6 bg-success rounded mr10 px-2 py-1">Công nghệ thông tin</small>
                                <small class="bold-6 bg-success rounded mr10 px-2 py-1">Marketing</small>
                                <small class="bold-6 bg-success rounded mr10 px-2 py-1">XNK</small>
                            </div>
                            <div class="name ellipsis">@item.BusinessUser.BusinessName</div>
                            <p class="summary ellipsis-3 mb-0">
                                NashTech is part of Harvey Nash Group, the leading global
                                provider of talent and technology solutions. We are experts in technology, delivering
                                smart solutions that solve business challenges and create value. Our award-winning teams
                                apply deep expertise and passion to deliver complex IT projects globally.
                            </p>
                            <div class="text-center mt10">
                                <div class="btn btn-error">
                                    <a href="@Url.Action("Details", "MOUs",  new { id=item.ID })">
                                        Xem Thêm
                                    </a>
                                   
                                </div>
                            </div>
                        </div>
                    </div>
                }


            </div>
        </div>
    </section>


